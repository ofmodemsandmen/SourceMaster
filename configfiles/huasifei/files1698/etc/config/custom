
config bwallocate 'bwallocate'
	option rollover '1'
	option allocate '1000'
	option lock '0'
	option enabled '0'

config menu 'menu'
	option password '1234'
	option full '1'
	option enabled '1'
	option default '1'

config atcmd 'atcmd'
	option lock '0'
	option enabled '1'
	option sierra '/etc/sierracmd'
	option quectel '/etc/quectelcmd'
	option generic '/etc/genericcmd'
	
config bandlock 'bandlock'
	option enabled '1'
	option cenable '0'
	option earfcn '0'
	option pci '0'

config zerotier 'zerotier'
	option lock '0'
	option networkid 'xxxxxxxxxxxxxxxx'
	
config logo 'logo'
	option size 'small'