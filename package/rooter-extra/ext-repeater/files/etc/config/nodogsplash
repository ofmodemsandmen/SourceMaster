
config nodogsplash 'nodogsplash'
	option fwhook_enabled '0'
	option gatewayinterface 'br-lan'
	option gatewayname 'OpenWrt Nodogsplash'
	option maxclients '250'
	option preauthidletimeout '30'
	option authidletimeout '120'
	option sessiontimeout '1200'
	option checkinterval '600'
	list authenticated_users 'allow all'
	list users_to_router 'allow tcp port 22'
	list users_to_router 'allow tcp port 23'
	list users_to_router 'allow tcp port 53'
	list users_to_router 'allow udp port 53'
	list users_to_router 'allow udp port 67'
	list users_to_router 'allow tcp port 80'
	option enabled '0'
	option binauth '/etc/nodogsplash/auth.sh'

config password 'password'
	option max '6'
	list password 'admin0'
	list password 'admin1'
	list password 'admin2'
	list password 'admin3'
	list password 'admin4'
	list password 'admin5'
	option counter '2'

